    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^8.0.1",
    "mongodb-memory-server": "^6.6.1",
    "nodemon": "^2.0.4",
    "onchange": "^7.0.2",
    "snyk": "^1.354.0"
  }
